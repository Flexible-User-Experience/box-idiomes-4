{
    "name": "flux/box-idiomes",
    "description": "A Symfony 7.4 LTS project to manage Box Idiomes Enterprise",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-zip": "*",
        "artgris/filemanager-bundle": "^2.10",
        "beberlei/doctrineextensions": "^1.5",
        "digitick/sepa-xml": "^1.6",
        "doctrine/dbal": "^3.10",
        "doctrine/doctrine-bundle": "^2.18",
        "doctrine/doctrine-migrations-bundle": "^3.5",
        "doctrine/orm": "^3.5",
        "drewm/mailchimp-api": "^2.5",
        "egulias/email-validator": "^4.0",
        "erusev/parsedown": "^1.7",
        "excelwebzone/recaptcha-bundle": "^1.5",
        "friendsofsymfony/ckeditor-bundle": "^2.6",
        "friendsofsymfony/jsrouting-bundle": "^3.5",
        "knplabs/knp-paginator-bundle": "^6.9",
        "league/html-to-markdown": "^5.1",
        "liip/imagine-bundle": "^2.15",
        "nelexa/zip": "^4.0",
        "phpdocumentor/reflection-docblock": "^5.6",
        "phpoffice/phpspreadsheet": "^1.30",
        "phpstan/phpdoc-parser": "^1.33",
        "qipsius/tcpdf-bundle": "^2.0",
        "sonata-project/admin-bundle": "^4.39",
        "sonata-project/doctrine-orm-admin-bundle": "^4.19",
        "stof/doctrine-extensions-bundle": "^1.14",
        "symfony/asset": "7.3.*",
        "symfony/asset-mapper": "7.3.*",
        "symfony/console": "7.3.*",
        "symfony/doctrine-messenger": "7.3.*",
        "symfony/dotenv": "7.3.*",
        "symfony/event-dispatcher": "7.3.*",
        "symfony/expression-language": "7.3.*",
        "symfony/flex": "^2.9",
        "symfony/form": "7.3.*",
        "symfony/framework-bundle": "7.3.*",
        "symfony/http-client": "7.3.*",
        "symfony/intl": "7.3.*",
        "symfony/mailer": "7.3.*",
        "symfony/messenger": "7.3.*",
        "symfony/mime": "7.3.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/process": "7.3.*",
        "symfony/property-access": "7.3.*",
        "symfony/property-info": "7.3.*",
        "symfony/runtime": "7.3.*",
        "symfony/security-bundle": "7.3.*",
        "symfony/security-http": "7.3.*",
        "symfony/serializer": "7.3.*",
        "symfony/stimulus-bundle": "^2.31",
        "symfony/string": "7.3.*",
        "symfony/translation": "7.3.*",
        "symfony/twig-bundle": "7.3.*",
        "symfony/ux-chartjs": "^2.31",
        "symfony/ux-dropzone": "^2.31",
        "symfony/ux-turbo": "^2.31",
        "symfony/validator": "7.3.*",
        "symfony/web-link": "7.3.*",
        "symfony/yaml": "7.3.*",
        "symfonycasts/reset-password-bundle": "^1.23",
        "symfonycasts/sass-bundle": "^0.8",
        "tattali/calendar-bundle": "^1.3",
        "tecnickcom/tcpdf": "^6.10",
        "twig/cssinliner-extra": "^3.22",
        "twig/extra-bundle": "^3.22",
        "twig/inky-extra": "^3.22",
        "twig/markdown-extra": "^3.22",
        "twig/twig": "^3.22",
        "vich/uploader-bundle": "^2.8"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.89",
        "hautelook/alice-bundle": "^2.16",
        "phpunit/phpunit": "^12.4",
        "rector/rector": "^1.2",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "7.3.*",
        "symfony/css-selector": "7.3.*",
        "symfony/debug-bundle": "7.3.*",
        "symfony/maker-bundle": "^1.64",
        "symfony/stopwatch": "7.3.*",
        "symfony/web-profiler-bundle": "7.3.*"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "preferred-install": {
            "*": "dist"
        },
        "bump-after-update": false,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "ckeditor:install --tag=4.22.1 --clear=drop": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd",
            "sass:build": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "7.3.*"
        }
    }
}

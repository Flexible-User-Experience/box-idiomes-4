{
    "name": "flux/box-idiomes",
    "description": "A Symfony 5.4 LTS project to manage Box Idiomes Enterprise",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-zip": "*",
        "artgris/filemanager-bundle": "^2.0",
        "beberlei/doctrineextensions": "^1.3",
        "digitick/sepa-xml": "^1.6",
        "doctrine/annotations": "^1.0",
        "doctrine/dbal": "^3.1",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.10",
        "drewm/mailchimp-api": "^2.5",
        "egulias/email-validator": "^2.1",
        "excelwebzone/recaptcha-bundle": "^1.5",
        "friendsofsymfony/ckeditor-bundle": "^2.3",
        "friendsofsymfony/jsrouting-bundle": "^2.7",
        "gremo/zurb-ink-bundle": "dev-master",
        "liip/imagine-bundle": "^2.5",
        "phpdocumentor/reflection-docblock": "^5.2",
        "presta/sitemap-bundle": "^2.1",
        "qipsius/tcpdf-bundle": "^2.0",
        "saad-tazi/g-chart-bundle": "^3.1",
        "sensio/framework-extra-bundle": "^5.5",
        "sonata-project/admin-bundle": "^3.70",
        "sonata-project/doctrine-orm-admin-bundle": "^3.10",
        "sonata-project/exporter": "^2.3.0",
        "sonata-project/user-bundle": "^4.0",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/intl": "4.4.*",
        "symfony/mailer": "4.4.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/process": "4.4.*",
        "symfony/property-access": "4.4.*",
        "symfony/property-info": "4.4.*",
        "symfony/proxy-manager-bridge": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer": "4.4.*",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "4.4.*",
        "tattali/calendar-bundle": "^1.2",
        "tecnickcom/tcpdf": "^6.4",
        "twig/cssinliner-extra": "^3.0",
        "twig/extra-bundle": "^3.0",
        "twig/inky-extra": "^3.0",
        "twig/markdown-extra": "^3.0",
        "twig/twig": "^3.0",
        "vich/uploader-bundle": "^1.17"
    },
    "require-dev": {
        "hautelook/alice-bundle": "^2.9",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "^4.4",
        "symfony/css-selector": "^4.4",
        "symfony/debug-bundle": "^4.4",
        "symfony/phpunit-bridge": "5.4.*",
        "symfony/stopwatch": "^4.4",
        "symfony/web-profiler-bundle": "^4.4",
        "thibautselingue/local-php-security-checker-installer": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "local-php-security-checker-installer && local-php-security-checker"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "local-php-security-checker"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.4.*"
        }
    }
}
